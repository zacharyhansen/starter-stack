{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "@snaplet/seed": {
    "config": "seed.client.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "codegen": "npx kanel && npx supabase gen types typescript --db-url=$DATABASE_URL > ../web/lib/database.types.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate": "node --env-file=.env -r ts-node/register ./src/migrations/migrate.ts",
    "postmigrate": "npx @snaplet/seed sync",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "seed": "knex seed:run --knexfile=src/migrations/knexFiles/public.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clerk/backend": "^1.3.2",
    "@clerk/clerk-sdk-node": "^5.0.15",
    "@google-cloud/secret-manager": "^5.6.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "5.20.0",
    "@trpc/server": "^11.0.0-rc.553",
    "knex": "^3.1.0",
    "kysely": "^0.27.4",
    "nestjs-trpc": "^1.4.0",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@kristiandupont/recase": "^1.3.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/postgres-types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "^0.98.0",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.28",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.14.8",
    "@types/pg": "^8.11.10",
    "@vitest/coverage-v8": "^2.1.1",
    "adapter-pg": "link:@snaplet/seed/adapter-pg",
    "eslint": "8.57.0",
    "kanel": "3.10.1",
    "kanel-kysely": "^0.5.0",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "supabase": "^1.204.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "unplugin-swc": "1.5.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "^2.1.1",
    "vitest-mock-extended": "^2.0.2"
  }
}
